/*
 * usb@fc000000
 * usbdrd_dwc3_0 in bsp kernel
 * or usb_host0_xhci in mainline kernel
 */
&usbdrd_dwc3_0 {
	status = "okay";
	dr_mode = "otg";
	usb-role-switch;

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		dwc3_0_role_switch: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usbc0_role_sw>;
		};
	};
};

